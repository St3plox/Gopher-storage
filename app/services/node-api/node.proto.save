syntax = "proto3";

package node-api;

import "google/protobuf/any.proto";
import "google/protobuf/empty.proto";

option go_package = "github.com/St3plox/Gopher-storage/app/services/node-api;";

service NodeV1 {
  rpc Create(CreateRequest) returns (CreateResponse);
  rpc Get(GetRequest) returns (GetResponse);
}

message CreateRequest {
  string key = 1;
  google.protobuf.Any val = 2;
}

message GetRequest {
  string key = 1;
}

message GetResponse {
  google.protobuf.Any val = 1;
}
